    <ReplaceRegex>&lt;div data-oembed-url="https://vimeo.com/${vimeoId}" class="embed-responsive embed-responsive-16by9"&gt;&lt;iframe class="embed-responsive-item" src="http://player.vimeo.com/video/${vimeoId}?show_title=1&amp;amp;show_byline=1&amp;amp;show_portrait=1&amp;amp;&amp;amp;fullscreen=1"&gt;&lt;/iframe&gt;&lt;/div&gt;&lt;p&gt;&lt;/p&gt;</ReplaceRegex>
    <Variables>prefix;vimeoId</Variables>
    <UseModule>false</UseModule>
    <UseToolbar>true</UseToolbar>
    <ModuleClass></ModuleClass>
    <ExecOrder>1</ExecOrder>
  </YafBBCode>
