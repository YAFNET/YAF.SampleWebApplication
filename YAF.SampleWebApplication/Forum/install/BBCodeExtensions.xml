    <UseModule>true</UseModule>
    <UseToolbar>false</UseToolbar>
    <ModuleClass>YAF.Web.BBCodes.SpoilerBBCodeModule</ModuleClass>
    <ExecOrder>50</ExecOrder>
  </YafBBCode>
