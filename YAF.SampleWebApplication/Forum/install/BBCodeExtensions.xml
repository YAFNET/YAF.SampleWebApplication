<YafBBCodeList>
  <YafBBCode>
    <Name>TWITTER</Name>
    <Description>[TWITTER][localization=TWITTER_DESCRIPTION]Enter the Status URL of the Tweet you want to embed[/localization][/TWITTER]</Description>
    <OnClickJS></OnClickJS>
    <DisplayJS></DisplayJS>
    <EditJS></EditJS>
    <DisplayCSS></DisplayCSS>
    <SearchRegex>\[twitter\](?&lt;statusID&gt;.+?)\[/twitter\]</SearchRegex>
    <ReplaceRegex>&lt;div class="embed-responsive embed-responsive-1by1" data-oembed-url="${statusID}"&gt;&lt;iframe class="embed-responsive-item" src="https://twitframe.com/show?url=${statusID}"&gt;&lt;/iframe&gt;&lt;/div&gt;&lt;p&gt;&lt;/p&gt;</ReplaceRegex>
    <Variables>statusID</Variables>
    <UseModule>false</UseModule>
    <UseToolbar>true</UseToolbar>
    <ModuleClass></ModuleClass>
    <ExecOrder>100</ExecOrder>
  </YafBBCode>
  <YafBBCode>
    <Name>DAILYMOTION</Name>
    <Description>[dailymotion][localization=DAILYMOTION_DESCRIPTION]Enter Dailymotion Url (http://www.dailymotion.com/video/xxxx)[/localization][/dailymotion]</Description>
    <OnClickJS></OnClickJS>
    <DisplayJS></DisplayJS>
    <EditJS></EditJS>
    <DisplayCSS></DisplayCSS>
    <SearchRegex>\[dailymotion\](?&lt;inner&gt;http://(?&lt;prefix&gt;[A-Za-z][A-Za-z][A-Za-z]?\.)?dailymotion.com/video/(?&lt;id&gt;[^_]*))[^[]*\[/dailymotion\]</SearchRegex>
    <ReplaceRegex>&lt;div data-oembed-url="https://dailymotion.com/video/${id}" class="embed-responsive embed-responsive-1by1"&gt;&lt;iframe class="embed-responsive-item" src="https://dailymotion.com/embed/video/${id}?forcedQuality=hd720"&gt;&lt;/iframe&gt;&lt;/div&gt;&lt;p&gt;&lt;/p&gt;</ReplaceRegex>
    <Variables>id;prefix</Variables>
    <UseModule>false</UseModule>
    <UseToolbar>false</UseToolbar>
    <ModuleClass></ModuleClass>
    <ExecOrder>1</ExecOrder>
  </YafBBCode>
  <YafBBCode>
    <Name>YOUTUBE</Name>
    <Description>[YOUTUBE][localization=YOUTUBEMOD_DESCRIPTION]Put YouTube URL Here[/localization][/YOUTUBE]</Description>
    <OnClickJS></OnClickJS>
    <DisplayJS></DisplayJS>
    <EditJS></EditJS>
    <DisplayCSS></DisplayCSS>
    <SearchRegex>\[youtube\](?&lt;inner&gt;(?&lt;prefix&gt;.+?)youtube.com/watch\?v=(?&lt;id&gt;[0-9A-Za-z-_]{11}))[^[]*\[/youtube\]|\[youtube\](?&lt;inner&gt;(?&lt;prefix&gt;.+?)youtu.be/(?&lt;id&gt;[0-9A-Za-z-_]{11}))[^[]*\[/youtube\]</SearchRegex>
    <ReplaceRegex>
      &lt;div data-oembed-url="//youtube.com/embed/${id}" class="embed-responsive embed-responsive-16by9"&gt;
      &lt;iframe src="//youtube.com/embed/${id}?hd=1" class="embed-responsive-item"&gt;&lt;/iframe&gt;&lt;/div&gt;&lt;p&gt;&lt;/p&gt;
    </ReplaceRegex>
    <Variables>id</Variables>
    <UseModule>false</UseModule>
    <UseToolbar>true</UseToolbar>
    <ModuleClass></ModuleClass>
    <ExecOrder>1</ExecOrder>
  </YafBBCode>
  <YafBBCode>
    <Name>SPOILER</Name>
    <Description>[SPOILER][localization=SPOILERMOD_DESCRIPTION]Put Spoiler Text Here[/localization][/SPOILER]</Description>
    <OnClickJS></OnClickJS>
