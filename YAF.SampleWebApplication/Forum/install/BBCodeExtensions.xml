  <YafBBCode>
    <Name>VIMEO</Name>
    <Description>[VIMEO]http://www.vimeo.com/xxxxxxx[/VIMEO]</Description>
    <OnClickJS></OnClickJS>
    <DisplayJS></DisplayJS>
    <EditJS></EditJS>
    <DisplayCSS></DisplayCSS>
